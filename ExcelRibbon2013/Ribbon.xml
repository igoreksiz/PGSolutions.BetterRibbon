<!-- When debugging, remember to check: Options -> Advanced -> General -> Show_Add-In_user_interface_errors. -->
<mso:customUI xmlns:mso="http://schemas.microsoft.com/office/2009/07/customui"
              xmlns:rd="ExcelRibbon2013"
              onLoad="OnRibbonLoad" loadImage="Ribbon_LoadImage">
  <mso:ribbon>
    <mso:tabs>
      <mso:tab idQ="rd:TabPGSolutions" label="PGSolutions" >
        <mso:group id="BrandingGroup" getLabel="getLabel" getKeytip="getKeytip" getVisible="getVisible">
          <mso:button id="BrandingButton" getImage="getImage"
                      getLabel="getLabel" getKeytip="getKeytip" getScreentip="getScreentip" getSupertip="getSupertip"
                      onAction="onAction" getVisible="getVisible" getEnabled="getEnabled" getSize="getSize"
                      getShowImage="getShowImage" getShowLabel="getShowLabel"
                     />
        </mso:group>
        <mso:group id="StandardButtonsGroup" getLabel="getLabel" getKeytip="getKeytip" getVisible="getVisible">
          <mso:toggleButton id="ShowAdvancedToggle" getImage="getImage"
                      getLabel="getLabel" getKeytip="getKeytip" getScreentip="getScreentip" getSupertip="getSupertip"
                      onAction="onActionToggle" getVisible="getVisible" getEnabled="getEnabled" getPressed="getPressed"
                      getSize="getSize" getShowImage="getShowImage" getShowLabel="getShowLabel"
                     />
          <mso:button id="StandardButton1" getImage="getImage"
                      getLabel="getLabel" getKeytip="getKeytip" getScreentip="getScreentip" getSupertip="getSupertip"
                      onAction="onAction" getVisible="getVisible" getEnabled="getEnabled" getSize="getSize"
                      getShowImage="getShowImage" getShowLabel="getShowLabel"
                     />
          <mso:button id="StandardButton2" getImage="getImage"
                      getLabel="getLabel" getKeytip="getKeytip" getScreentip="getScreentip" getSupertip="getSupertip"
                      onAction="onAction" getVisible="getVisible" getEnabled="getEnabled" getSize="getSize"
                      getShowImage="getShowImage" getShowLabel="getShowLabel"
                     />
          <!-- Use this DropDown template when the item list will be statically defined in this file. -->
          <mso:dropDown id="ButtonOptions"
                        getLabel="getLabel" getKeytip="getKeytip" getScreentip="getScreentip" getSupertip="getSupertip"
                        getVisible="getVisible" getEnabled="getEnabled"
                        onAction="onActionDropDown" getSelectedItemIndex="getSelectedItemIndex"
                      >
            <mso:item id="LabelOnly" label="Label Only" screentip="Display Button Label Only"
                      supertip="Displays just the button label when its size is Regular; else both."
                      imageMso = "MacroSecurity"/>
            <mso:item id="ImageOnly" label="Image Only" screentip="Display Button Image Only"
                      supertip="Displays just the button image when its size is Regular; else both."
                      imageMso = "MacroSecurity"/>
            <mso:item id="LabelAndImage" label="Label &amp; Image" screentip="Display Label &amp; Image"
                      supertip="Always displays both the button label and image."
                      imageMso = "MacroSecurity"/>
          </mso:dropDown>
        </mso:group>
        <mso:group id="CustomButtonsGroup" getLabel="getLabel" getKeytip="getKeytip" getVisible="getVisible">
          <mso:box id="CustomButtonsBox1" boxStyle="horizontal" visible="true" >
            <mso:toggleButton id="SizeToggle" getImage="getImage"
                      getLabel="getLabel" getKeytip="getKeytip" getScreentip="getScreentip" getSupertip="getSupertip"
                      onAction="onActionToggle" getVisible="getVisible" getEnabled="getEnabled" getPressed="getPressed"
                      getSize="getSize" getShowImage="getShowImage" getShowLabel="getShowLabel"
                     />
            <!-- Use this DropDown template when the item list will be dynamically defined through callbacks. -->
            <mso:dropDown id="ButtonOptions2"
                          getLabel="getLabel" getKeytip="getKeytip" getScreentip="getScreentip" getSupertip="getSupertip"
                          getVisible="getVisible" getEnabled="getEnabled"
                          getItemCount="getItemCount" getItemID="getItemId" getItemLabel="getItemLabel"
                          getItemScreentip="getItemScreentip" getItemSupertip="getItemSupertip" getItemImage="getItemImage"
                          onAction="onActionDropDown" getSelectedItemID="getSelectedItemID"
                          >
            </mso:dropDown>
          </mso:box>
          <mso:box id="CustomButtonsBox2" boxStyle="vertical" visible="true" >
            <mso:button id="AppLaunchButton1" getImage="getImage"
                        getLabel="getLabel" getKeytip="getKeytip" getScreentip="getScreentip" getSupertip="getSupertip"
                        onAction="onAction" getVisible="getVisible" getEnabled="getEnabled" getSize="getSize"
                        getShowImage="getShowImage" getShowLabel="getShowLabel"
                       />
            <mso:button id="AppLaunchButton2" getImage="getImage"
                        getLabel="getLabel" getKeytip="getKeytip" getScreentip="getScreentip" getSupertip="getSupertip"
                        onAction="onAction" getVisible="getVisible" getEnabled="getEnabled" getSize="getSize"
                        getShowImage="getShowImage" getShowLabel="getShowLabel"
                       />
            <mso:button id="AppLaunchButton3" getImage="getImage"
                        getLabel="getLabel" getKeytip="getKeytip" getScreentip="getScreentip" getSupertip="getSupertip"
                        onAction="onAction" getVisible="getVisible" getEnabled="getEnabled" getSize="getSize"
                        getShowImage="getShowImage" getShowLabel="getShowLabel"
                       />
          </mso:box>
        </mso:group>
        <mso:group id="UnknownGroup" getLabel="getLabel" getKeytip="getKeytip" getVisible="getVisible">
          <mso:button id="UnknownButton" imageMso="MacroPlay"
                      getLabel="getLabel" getKeytip="getKeytip" getScreentip="getScreentip" getSupertip="getSupertip"
                      onAction="onAction" getVisible="getVisible" getEnabled="getEnabled" getSize="getSize"
                      getShowImage="getShowImage" getShowLabel="getShowLabel"
                      />
        </mso:group>
      </mso:tab>
    </mso:tabs>
  </mso:ribbon>
</mso:customUI>
